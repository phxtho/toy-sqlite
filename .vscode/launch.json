  "version": "0.2.0",
  "configurations": [
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug unit tests in library 'sqlite-starter-rust'",
      "cargo": {
        "args": ["test", "--no-run", "--lib", "--package=sqlite-starter-rust"],
        "filter": {
          "name": "sqlite-starter-rust",
          "kind": "lib"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug executable 'sqlite-starter-rust'",
      "cargo": {
        "args": [
          "build",
          "--bin=sqlite-starter-rust",
          "--package=sqlite-starter-rust"
        ],
        "filter": {
          "name": "sqlite-starter-rust",
          "kind": "bin"
        }
      },
      "args": ["sample.db", ".table"],
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug unit tests in executable 'sqlite-starter-rust'",
      "cargo": {
        "args": [
          "test",
          "--no-run",
          "--bin=sqlite-starter-rust",
          "--package=sqlite-starter-rust"
        ],
        "filter": {
          "name": "sqlite-starter-rust",
          "kind": "bin"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    }
  ]
}
